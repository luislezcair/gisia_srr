@model List<string>

@{
    ViewBag.Title = "Claves de búsqueda";
}

<div class="page-header">
    <h2>Claves de búsqueda</h2>
</div>

<table class="table table-bordered table-hover table-striped">
    <thead>
        <tr>
            <th style="width: 3%">#</th>
            <th>Clave</th>
        </tr>
    </thead>
    <tbody>
        @{ int i = 0; }
        @foreach (var s in Model)
        {
            <tr>
                <td>@(++i)</td>
                <td>@s</td>
            </tr>
        }
    </tbody>
</table>
